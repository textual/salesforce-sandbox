<!DOCTYPE html>
<html>
<head>
  <title>force api</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
  <div id="user_nav"> 
    <%= link_to "Home", :root %> | 
    <% if @logged_in %>
      <%= link_to "Log Out", "/auth/destroy" %> |
      <%= link_to "Accounts", :controller => "home", :action => "contacts"%> |
      <%= link_to "Contracts", :controller => "home", :action => "contracts"%> |
      <%= link_to "dump", :controller => "home", :action => "dump"%> |
    <% else %>
      <%= link_to "Log In SalesForce", "/auth/forcedotcom" %> |
    <% end %>
    <%= link_to "Tasks", "/tasks" %>
    
  </div>
	<hr/>
	<%= yield %>
	
</body>
</html>
